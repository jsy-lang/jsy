    "rollup-plugin-terser": "^1.0.1",
    "source-map": "^0.7.3",
    "jsy-transpile": "file:."
  },
  "babel": {
    "plugins": [
      "offside-js"
    ]
  },
  "scripts": {
    "clean": "rm -rf cjs/ umd/ esm/",
    "build": "rollup --config",
    "watch": "npm -s run build -- --watch",
    "pretest": "npm -s run build",
    "test": "cd test; npm -s run test",
    "test:install": "cd test; npm -s install",
