{
  "name": "jsy-transpile",
  "version": "0.10.1",
  "description": "Offside (indention) JSY syntax transpiler to standard JavaScript â€” without Babel",
  "author": "Shane Holloway <shane.holloway@ieee.org>",
  "license": "BSD-2-Clause",
  "keywords": [
    "syntax",
    "offside",
    "python",
    "indention",
    "indent"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsy-lang/jsy-transpile.git"
  },
  "bugs": {
    "url": "https://github.com/jsy-lang/jsy-transpile/issues"
  },
  "homepage": "https://github.com/jsy-lang/jsy-transpile#readme",
  "bin": {
    "jsy-transpile": "transpile.mjs",
    "jsy-scan": "scan.mjs"
  },
  "files": [
    "esm",
    "code",
    "jsy-script.js",
    "transpile.mjs",
    "scan.mjs",
    "unittest.html",
    "test/__unittest.iife.js"
  ],
  "type": "module",
  "module": "esm/index.js",
  "browser": "esm/index.js",
  "exports": {
    "./*": "./*",
    ".": "./esm/index.js"
  },
  "workspaces": [
    "test/"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-virtual": "^3.0.1",
    "@rollup/pluginutils": "^5.0.2",
    "micromatch": "^4.0.5",
    "rollup": "^3.17.2",
    "rollup-plugin-dgnotify": "^0.1.8",
    "source-map": "^0.7.4",
    "tiny-source-map": "^0.8.0"
  },
  "scripts": {
    "clean": "rm -rf umd/ esm/ iife/",
    "build": "rollup --config",
    "build:stable": "rollup --config rollup.stable.js",
    "watch:fast": "JSY_BUILD=fast npm -s run build -- --watch",
    "watch": "npm -s run build -- --watch",
    "pretest": "npm -s run build",
    "test": "cd test; npm -s run test",
    "test:install": "cd test; npm -s install",
    "test:one": "cd test; npm -s run test:one",
    "test:lean": "cd test; npm -s run test:lean",
    "test:watch": "cd test; npm -s run test:watch",
    "prepublishOnly": "npm -s run clean && npm -s run test"
  }
}
