  "browser": "umd/jsy-transpile.js",
  "dependencies": {},
  "devDependencies": {
    "acorn": "^5.7.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-offside-js": "^0.10.3",
    "rollup": "^0.64.1",
