    "indent"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsy-lang/jsy-transpile.git"
  },
  "bugs": {
    "url": "https://github.com/jsy-lang/jsy-transpile/issues"
  },
